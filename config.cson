  hasklig:
    haskligWeight: "regular"
  "language-elm":
    autocompleteEnabled: false
  linter: {}
  "linter-elm-make":
    alwaysCompileMain: true
  "linter-ui-default":
    panelHeight: 69
    panelRepresents: "Entire Project"
    showPanel: true
    statusBarClickBehavior: "Jump to next issue"
    statusBarRepresents: "Entire Project"
    tooltipFollows: "Mouse"
