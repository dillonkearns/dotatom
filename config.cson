    autocompleteMinChars: 2
    autocompleteSnippetsEnabled: true
    cacheDirectory: "/var/folders/rp/2jl5jpgs565fdvks2gtfkrd00000gn/T/elmjutsu52995uwKfg003zyyW"
    globalAutocompleteEnabled: true
    hyperclickEnabled: true
    showAssociativitiesInSidekick: false
