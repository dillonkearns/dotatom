    hidePanelHeading: true
    panelVisibility: "Keep Visible"
    stealFocus: false
  "copy-with-style":
    preset1:
      font_family: "hasklig"
      format: "RTF"
  core:
    customFileTypes:
      "source.ini": [
        ".buckconfig"
        ".hgrc"
        ".flowconfig"
      ]
      "source.json": [
        ".arcconfig"
        "BUCK.autodeps"
      ]
      "source.python": [
        "BUCK"
      ]
    disabledPackages: [
      "vim-mode"
      "minitest-snippets"
      "ruby-block"
      "ruby-test"
      "atom-elixir"
      "language-elixir"
      "linter-elixir-credo"
      "minimap"
      "atom-typescript"
      "tree-view"
      "linter"
    ]
    telemetryConsent: "limited"
    themes: [
      "one-dark-ui"
      "atom-dark-syntax"
    ]
    titleBar: "hidden"
  "cyber-dojo":
    workspace: "/Users/dillon/Dropbox/opensource/katas/cyberdojo"
  editor:
    fontFamily: "Hasklig"
    fontSize: 13
    invisibles: {}
    showIndentGuide: true
  elmjutsu:
